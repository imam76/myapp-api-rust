  "query": "\n            SELECT w.id, w.name, w.description, w.owner_id, w.created_by, w.updated_by, w.created_at, w.updated_at,\n                   wu.role as \"role!: WorkspaceRole\"\n            FROM workspaces w\n            JOIN workspace_users wu ON w.id = wu.workspace_id\n            WHERE wu.user_id = $1\n            ORDER BY w.name\n            ",
